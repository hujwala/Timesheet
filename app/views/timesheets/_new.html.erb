
<div class="row" > 
 <%= form_for :time_sheet, url: timesheets_path, method: :post, html: {:id =>"time_sheet",:class =>"form-horizontal autosave"} do |f| %>
 <%#= form_for :timesheet, url: timesheets_path, method: :post, html: {:id =>"time_sheet",:class =>"form-horizontal"} do |f| %>

 <!-- <form class="form-horizontal"> -->
   <!--Timesheet row -->
   <div class="col-sm-12">
     <div class="form-group alert alert-default alert-dismissible fade in">
      <div class="col-sm-3">
        <%= f.text_field :project_name, :class =>"form-control",:id =>"Name", :placeholder => "project" %> </div>
        <div class="col-sm-6">
         <%= f.text_area :description, :class =>"form-control",:id =>"description", :placeholder => "Task completed" %> 
       </div>
       <div class="col-sm-2">
        <%= f.text_field :working_time, :class =>"form-control",:id =>"working_time", :placeholder => "hh:mm" %> 
      </div>

    </div>

  </div>
  <% end %>
  <!--./Timeshow row  -->
</div>

