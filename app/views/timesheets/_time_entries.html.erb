<table class="table table-condensed">
  <thead>
      <tr>
      <th>Date</th>
      <th>Project Name</th>
      <th>Description</th>
      <th>Working Time</th>
      <th></th>
    </tr>
  </thead>
  <tbody id="tableData">
    <% @time_sheets.each do |time_sheet| %>
      <tr>
        <td><%= time_sheet.date %></td>
        <td><%= time_sheet.project_name %></td>
        <td><%= truncate(time_sheet.description, length: 5) %></td>
        <td><%= time_sheet.working_time %></td>
        <td><%= link_to "Edit", edit_timesheet_path(time_sheet),:remote => true %></td>
      </tr>
    <% end %>
  </tbody>
</table>
