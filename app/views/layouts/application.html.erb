<!DOCTYPE html>
<html>
<head>

	<title>Timesheet</title>
	
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
</head>
<body>
    
	<%= render 'layouts/common/header' unless @disable_nav %> 

	<div id="div_flash_message" class="container">
		<%= render :partial=>"/layouts/common/flash_messages" -%>
	</div>
	<%= yield %>
	<div id="popup_modal" class="modal fade"></div>
</body>
</html>
